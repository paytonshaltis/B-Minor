scores : array [10] integer = {97, 87, 83, 99, 77, 80, 72, 100, 92, 88}; 

main: function void () = {

	i : integer = 0;
	total : integer = 0;
	avg : integer = 0;

	// computes the average test score
	for(i = 0; i < 10; i++) {
		total = total + scores[i];
	}
	avg = total / 10;

	print "The average test score is ", avg, "\n";
	return;
}

