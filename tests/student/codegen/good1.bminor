// testing basic integer arithmetic in a function


main : function integer (argc : integer, argv : array [] string) = {
    
    // declaring some integers, with and without initial values
    a : integer;
    b : integer = -49;

    // a should be 33 by the end
    a = 20;
    a = (a * 2) + (b / 7);

    print "This should be 33: ", a, "\n";

    // b should be 16 by the end
    b = -b;
    b = (b - 45) ^ 2;

    print "This should be 16: ", b, "\n";

    return 0;
}